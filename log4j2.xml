<?xml version="1.0" encoding="UTF-8"?>
<configuration status="INFO" name="log" packages="" >
    <Appenders>       
        
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} [db2sdp] %p &lt;%c&gt; &lt;%t&gt; %msg%n"/>
        </Console>
        
    </Appenders>

    <loggers>
        <root level="TRACE">            
            <AppenderRef ref="console" level="FATAL"/>
        </root>
    </loggers>
</configuration>
